{{!< default}}

{{#foreach posts}}

<section id="{{id}}" class="{{post_class}}">

	<h3 class="post-title">
		<a href="{{url}}">{{date published_at format='MMMM Do, YYYY'}} &mdash; {{title}}</a>
	</h3>

	<section class="post-excerpt">
        <p>{{excerpt}}</p>
    </section>

</section>

{{/foreach}}

{{pagination}}